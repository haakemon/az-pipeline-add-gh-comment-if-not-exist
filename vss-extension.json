{
  "manifestVersion": 1,
  "id": "github-add-pr-comment-if-not-exist",
  "name": "GitHub Add PR Comment if not exist",
  "version": "0.5.0",
  "publisher": "haakemon",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "galleryFlags": ["Public"],
  "description": "Add a comment to a GitHub PR only if the comment doesnt already exist.",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "files": [
    {
      "path": "src"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/haakemon/az-pipeline-add-gh-comment-if-not-exist"
  },
  "links": {
    "issues": {
      "uri": "https://github.com/haakemon/az-pipeline-add-gh-comment-if-not-exist/issues"
    }
  },
  "contributions": [
    {
      "id": "task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "src"
      }
    }
  ]
}
