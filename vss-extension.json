{
  "manifestVersion": 1,
  "id": "github-add-pr-comment-if-not-exist",
  "name": "GitHub Add PR Comment if not exist",
  "version": "0.1.16",
  "publisher": "haakemon",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Add a comment to a GitHub PR only if the comment doesnt already exist.",
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "src/images/extension-icon.png"
  },
  "content": {
    "details": {
      "path": "src/README.md"
    }
  },
  "files": [
    {
      "path": "src"
    }
  ],
  "repository": {
      "type": "git",
      "uri": "https://github.com/haakemon/az-pipeline-add-gh-comment-if-not-exist"
  },
  "links": {
      "repository": {
          "uri": "https://github.com/haakemon/az-pipeline-add-gh-comment-if-not-exist"
      },
      "issues": {
          "uri": "https://github.com/haakemon/az-pipeline-add-gh-comment-if-not-exist/issues"
      }
  },
  "contributions": [
    {
      "id": "custom-comment-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "src"
      }
    }
  ]
}
