{
  "manifestVersion": 1,
  "id": "github-add-pr-comment-if-not-exist",
  "name": "GitHub Add PR Comment if not exist",
  "version": "0.1.11",
  "publisher": "haakemon",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Add a comment to a GitHub PR only if the comment doesnt already exist.",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "src"
    }
  ],
  "contributions": [
    {
      "id": "custom-comment-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "src"
      }
    }
  ]
}